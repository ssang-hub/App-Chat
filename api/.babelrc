{
    "presets": ["@babel/preset-env"],
    "plugins": [
        [
            "module-resolver",
            {
                "root": ["./src"],
                "alias": {
                    "@config": "./src/config",
                    "@models": "./src/models",
                    "@controllers": "./src/controllers",
                    "@middlewares": "./src/middlewares",
                    "@routes": "./src/routes",
                    "@services": "./src/services",
                    "@socket" : "./src/socket",
                    "@views": "./src/views"
                }
            }
        ]
    ]
}
